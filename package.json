{
  "name": "specmgr",
  "version": "1.0.0",
  "description": "Local DocSearch & Chat Assistant - specification management system",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon --exec ts-node src/server/index.ts",
    "dev:frontend": "cd src/client && npm run dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc -p tsconfig.server.json",
    "build:frontend": "cd src/client && npm run build",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit && cd src/client && npm run typecheck",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "chokidar": "^3.5.3",
    "bullmq": "^4.15.4",
    "ioredis": "^5.3.2",
    "qdrant-js": "^1.7.0",
    "openai": "^4.24.1",
    "crypto": "^1.0.1",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "winston": "^3.11.0",
    "express-rate-limit": "^7.1.5",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/fs-extra": "^11.0.4",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "markdown",
    "documentation",
    "search",
    "chat",
    "vector-database",
    "qdrant",
    "llm",
    "typescript"
  ],
  "author": "Ryo HIGASIGAWA",
  "license": "MIT"
}